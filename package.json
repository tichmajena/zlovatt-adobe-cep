{
  "name": "zl-template-svelte",
  "version": "0.2.0",
  "private": true,
  "author": "zlovatt",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/zlovatt/zl-template-svelte.git"
  },
  "homepage": "https://lova.tt/",
  "scripts": {
    "start": "cross-env NODE_ENV=development gulp",
    "build": "cross-env NODE_ENV=production gulp build"
  },
  "cep": {
    "name": "Muruki",
    "id": "com.muruki.imajenation",
    "debugInProduction": true,
    "hosts": "AEFT, PHXS, PHSP",
    "extensions": [
      {
        "type": "Panel",
        "id": "com.muruki.imajenation.panel",
        "name": "Muruki",
        "htmlFilename": "./index.html",
        "panelWidth": 840,
        "panelHeight": 600,
        "cefParams": [
          "--allow-file-access-from-files",
          "--allow-file-access",
          "--enable-nodejs",
          "--mixed-context"
        ]
      }
    ]
  },
  "devDependencies": {
    "@tsconfig/svelte": "^1.0.10",
    "aequery": "^0.6.0",
    "autoprefixer": "^10.4.0",
    "cep-bundler-webpack": "0.0.58",
    "cep-interface": "^0.5.1",
    "chalk": "^4.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^2.1.1",
    "dotenv-webpack": "^2.0.0",
    "gnirts": "^1.1.7",
    "gnirts-loader": "^1.0.7",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "mini-css-extract-plugin": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^0.23.1",
    "svelte": "^3.24.1",
    "svelte-loader-hot": "^0.3.1-1",
    "svelte-preprocess": "^4.1.1",
    "tailwindcss": "^2.2.19",
    "ts-loader": "^8.0.3",
    "types-for-adobe": "bbb999/types-for-adobe",
    "typescript": "^4.0.2",
    "webpack": "^4.30.0",
    "webpack-dev-server": "^3.3.1",
    "webpack-stream": "^5.2.1",
    "write-file-webpack-plugin": "^4.5.1",
    "zxp-sign-cmd": "^2.0.0"
  },
  "dependencies": {
    "@adobe/spectrum-css": "^2.12.0",
    "gulp-postcss": "^9.0.1",
    "mkdirp": "^1.0.4",
    "path": "^0.12.7",
    "temp": "^0.9.1"
  }
}
